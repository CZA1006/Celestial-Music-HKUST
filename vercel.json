{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/location", "destination": "/frontend/location.html" },
    { "source": "/starmap", "destination": "/frontend/starmap.html" }
  ],
  "routes": [
    { "src": "/location", "dest": "/frontend/location.html" },
    { "src": "/starmap", "dest": "/frontend/starmap.html" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}
