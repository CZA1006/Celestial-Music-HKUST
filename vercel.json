{
  "rewrites": [
    { "source": "/", "destination": "/frontend/index.html" },
    { "source": "/location", "destination": "/frontend/location.html" },
    { "source": "/starmap", "destination": "/frontend/starmap.html" },
    { "source": "/config.js", "destination": "/frontend/config.js" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
